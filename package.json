{
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "eslint src/**/*.{ts,tsx} --fix && prettier **/*.{ts,tsx,json} --write",
    "test": "jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/?(*.)+(spec|test).+(js|ts|tsx)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "storybook-addon-state": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-actions": "^6.4.0-alpha.4",
    "@storybook/addon-docs": "^6.3.4",
    "@storybook/addon-essentials": "^6.4.0-alpha.4",
    "@storybook/addon-links": "^6.4.0-alpha.4",
    "@storybook/react": "^6.4.0-alpha.4",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.0",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "add": "^2.0.6",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.5",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "prop-types": "^15.7.2",
    "storybook-builder-vite": "^0.0.9",
    "tailwindcss": "^2.2.4",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4",
    "vite": "^2.3.8",
    "yarn": "^1.22.10"
  }
}
